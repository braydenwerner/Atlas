{
  "short_name": "Atlas",
  "name": "Atlas",
  "key": "",
  "description": "A comprehensive tool for drawing, taking notes, and staying organized all on your chrome homepage.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "version": "0.0.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": {
      "16": "./images/atlas-16.png",
      "48": "./images/atlas-48.png"
    }
  },
  "icons": {
    "16": "./images/atlas-16.png",
    "48": "./images/atlas-48.png",
    "128": "./images/atlas-128.png"
  }
}
